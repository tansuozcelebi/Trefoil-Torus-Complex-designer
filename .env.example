# Docker Environment Configuration
# Copy this file to .env and customize as needed

# Application Configuration
APP_NAME=trefoil-torus-complex
# Update this to match your release version, or use 'latest' for the most recent build
APP_VERSION=latest

# Docker Configuration
COMPOSE_PROJECT_NAME=trefoil-torus

# Container Port Mapping
# The port on your host machine that will map to the container's port 80
HOST_PORT=8080

# Node Environment (for build)
NODE_ENV=production
